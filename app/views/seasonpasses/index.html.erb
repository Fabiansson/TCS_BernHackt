<h1>Seasonpasses</h1>

<ul class="list-group">
  <% @seasonpasses.each do |seasonpass| %>
    <li class="list-group-item d-flex justify-content-between align-items-center">
      <%= link_to seasonpass.club.name, seasonpass_path(seasonpass) %>
      <%= link_to 'Delete Seasonpass', seasonpass, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
    </li>
  <% end %>
</ul>

<br>

<%= link_to 'Add Seasonpass', new_seasonpass_path, class: 'btn btn-primary' %>
